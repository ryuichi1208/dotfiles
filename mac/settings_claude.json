{
  "cleanupPeriodDays": 30,
  "env": {
    "ANTHROPIC_CUSTOM_HEADERS": "[object Object]",
    "BASH_DEFAULT_TIMEOUT_MS": "120000",
    "DISABLE_TELEMETRY": "false"
  },
  "permissions": {
    "allow": [
      "Read(~/.zshrc)",
      "List(*)",
      "Bash(ls:*)",
      "Bash(grep:*)",
      "Bash(awk:*)",
      "Bash(bat:*)",
      "Bash(cat:*)",
      "Bash(rg:*)",
      "Bash(find:*)",
      "Bash(sed:*)",
      "Bash(sort:*)",
      "Bash(uniq:*)",
      "Bash(uv sync)",
      "Bash(uv pip list)",
      "Bash(go version)",
      "Bash(go env)",
      "Bash(go mod tidy)",
      "Bash(perl --version)",
      "Bash(npm run lint)",
      "Bash(npm run test:*)",
      "Bash(actionlint:*)",
      "Bash(mkdir -p ./:*)",
      "Bash(rm hello.py)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:vercel.com)",
      "WebFetch(domain:docs.anthropic.com)",
      "WebFetch(domain:stackoverflow.com)",
      "WebFetch(domain:developer.mozilla.org)",
      "WebFetch(domain:npmjs.com)",
      "WebFetch(domain:nextjs.org)",
      "WebFetch(domain:docs.aws.amazon.com)"
    ],
    "deny": [
      "Write(~/.zshrc)",
      "Write(~/.vimrc)",
      "Write(~/.claude/settings.json)"
    ],
    "defaultMode": "plan"
  },
  "model": "opus",
  "feedbackSurveyState": {
    "lastShownTime": 1754053425310
